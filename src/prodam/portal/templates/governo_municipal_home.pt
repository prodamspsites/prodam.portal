<div class="tile-header">
                <h1>
                  GOVERNO MUNICIPAL
                </h1>
</div>
<div class="governo-lista" tal:define="governoMunicipal python:context.restrictedTraverse('governoMunicipal')">
  <h2> Encontre as secretarias</h2>
  <span tal:replace="structure governoMunicipal/getSecretarias"></span>
  <h2> Encontre as prefeituras regionais</h2>
  <span tal:replace="structure governoMunicipal/getSubPrefeituras"></span>
<!--
<div class="tile-footer">
    <a href="/agenda">Agenda do prefeito <span class="iconeMais"></span></a>
-->
  <div class="tile-footer" tal:define="agenda governoMunicipal/getAgenda; dia governoMunicipal/getDiaPrefeito">
     <a href="#" tal:condition="agenda" tal:attributes="href agenda/getRemoteUrl; target python: '_blank' if agenda.getObject().target == True else '_self'"   >
<tal:titulo  tal:content="string:${agenda/Title}">

</tal:titulo>

  <span class="iconeMais"></span>  </a>

<a href="#" style="border-top: none; margin-top: -30px;" tal:condition="dia" tal:attributes="href dia/getRemoteUrl; target python: '_blank' if dia.getObject().target == True else '_self'"   >

<tal:outrotitulo  tal:content="string:${dia/Title}">

</tal:outrotitulo>
  <span class="iconeMais"></span>  </a>

  </div>
</div>
