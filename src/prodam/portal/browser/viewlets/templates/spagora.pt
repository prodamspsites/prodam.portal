<aside id="externos" role="complementary" tal:define="spagora python:context.restrictedTraverse('sp-agora')">
  <header class="dash-header">
    SÃ£o Paulo agora:
    <button class="e-fechar"> </button>
  </header>

  <ul id="servicos-externos">
    <tal:servicos replace="structure python:spagora.getCapa()"></tal:servicos>
  </ul>

  <a href="/mapa" tal:attributes="href string:${context/portal_url}/mapa"><div id="mapa-serv"></div></a>

  <div class="tweets" tal:condition="python:spagora.getTweets()">
    <h4><a href="https://twitter.com/" target="_blank" class="labelsp selecionado">Alertas
         <span>@saopaulo_agora </span>
      </a>
    </h4>
    <div class="tweetlist">
      <tal:tweets define="tweets python:spagora.getTweets()" repeat="tweet tweets">
        <span tal:replace="structure tweet" />
      </tal:tweets>
        <button id="voltar" class="sprite-verao rw"> </button>
        <button id="ir" class="sprite-verao fw"> </button>
    </div>
  </div>
</aside>