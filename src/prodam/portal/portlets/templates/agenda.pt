<div class="prefeito">
<!--
  <h2 id="agendaPrefeitura"><a href="" tal:attributes="href string:${context/portal_url}/agenda"><span tal:replace="structure view/getPortletTitle" /></a></h2>
-->
<h2 id="agendaPrefeitura" tal:define="agenda view/getAgenda"><a href="" tal:attributes="href agenda/getRemoteUrl; target python: agenda.getObject().target"><span tal:replace="structure agenda/Title" /></a></h2>


</div>

<ul id="agenda" tal:define="events view/getEvents">
  <li tal:repeat="event events">
    <article>
      <time tal:content="python: str(event.start)[11:16]">11:00</time>
      <a tal:define="dayEvent view/getDayEvent;yearEvent view/getYearEvent; monthEvent view/getMonthEvent" tal:attributes="href python:event.getObject().absolute_url()+'?year='+yearEvent+'&month='+monthEvent+'&day='+dayEvent">
        <h3 tal:content="event/Title">Despachos internos</h3>
      </a>
      <p tal:content="event/Description"></p>
    </article>
  </li>
</ul>
<a href="#" tal:define="agenda view/getAgenda" tal:attributes="href agenda/getRemoteUrl;  target python: agenda.getObject().target" class="veragendaCompleta">Ver agenda completa <span class="sprite-arrowright2"></span></a>
